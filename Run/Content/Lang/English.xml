    <!-- 	Spell Commands		  -->
    <Item Key="CmdSpellDescription">
      <Value>Provides commands to interact with the Spells of the Target.</Value>
    </Item>

    <Item Key="CmdSpellAddDescription">
      <Value>
        Adds the given spell.
        -r (Reagents) switch also adds all constraints required by the Spell (Tools, Reagents, Objects, Skills).
        -c [&lt;class&gt;] adds all spells of the Character's or a given class.
        -t (talents) adds the highest rank of all spells of all talents
      </Value>
    </Item>
    <Item Key="CmdSpellAddParamInfo">
      <Value>[-[r][c [&lt;class&gt;]]] [list of &lt;spell|line|talent&gt;]</Value>
    </Item>
    <Item Key="CmdSpellAddResponseSpell">
      <Value>Spell added : {0}</Value>
    </Item>
    <Item Key="CmdSpellAddResponseSpells">
      <Value>Added {0} spells.</Value>
    </Item>
    <Item Key="CmdSpellAddResponseTalents">
      <Value>Added {0} talents.</Value>
    </Item>

