<Config>
  <Settings>
    <IRCAddon>
      <AccountAssociationsList>
        <AccountAssociationFileName>AccAssList.xml</AccountAssociationFileName>
      </AccountAssociationsList>
      <Commands>
        <CompactHelpCommand>
          <MaxUncompressedCmds>3</MaxUncompressedCmds>
        </CompactHelpCommand>
        <StatsCommand>
          <MuteChannelWhenPostingStats>False</MuteChannelWhenPostingStats>
        </StatsCommand>
        <VoteStartCommand>
          <VoteAnswerPrefix>@</VoteAnswerPrefix>
        </VoteStartCommand>
        <WCellCmdTrigger>
          <WCellCmdPrefixes>
            <Item>#</Item>
          </WCellCmdPrefixes>
        </WCellCmdTrigger>
      </Commands>
      <IrcAddonConfig>
        <Channels>
          <Item>#ChannelWithKey,ChannelKey</Item>
          <Item>#ChannelWithoutKey,</Item>
        </Channels>
        <ExceptionChan>#ExceptionChannel</ExceptionChan>
        <Info>ChangeMe</Info>
        <Network>euroserv.fr.quakenet.org</Network>
        <Nicks>
          <Item>Kiste</Item>
          <Item>Kiste2</Item>
          <Item>Kiste3</Item>
        </Nicks>
        <Port>6667</Port>
        <RequiredStaffPriv>HalfOp</RequiredStaffPriv>
        <UpdatedChannels>
          <Item>#ChannelName</Item>
        </UpdatedChannels>
        <UserName>Mokbot</UserName>
      </IrcAddonConfig>
      <IrcConnection>
        <AnnounceAuthToUser>True</AnnounceAuthToUser>
        <AuthAllUsersOnJoin>False</AuthAllUsersOnJoin>
        <AutoAuth>True</AutoAuth>
        <AutomaticTopicUpdating>True</AutomaticTopicUpdating>
        <EchoBroadcasts>True</EchoBroadcasts>
        <ExceptionChannelNotification>False</ExceptionChannelNotification>
        <ExceptionNotificationRank>1000</ExceptionNotificationRank>
        <ExceptionNotify>True</ExceptionNotify>
        <ExceptionNotifyStaffUsers>True</ExceptionNotifyStaffUsers>
        <HideChatting>False</HideChatting>
        <HideIncomingIrcPackets>False</HideIncomingIrcPackets>
        <HideOutgoingIrcPackets>False</HideOutgoingIrcPackets>
        <IrcCmdCallingRange>LocalChannel</IrcCmdCallingRange>
        <IrcCmdPrefixes>
          <Item>!</Item>
        </IrcCmdPrefixes>
        <ReConnectAttempts>100</ReConnectAttempts>
        <ReConnectOnDisconnect>True</ReConnectOnDisconnect>
        <ReConnectWaitTime>50</ReConnectWaitTime>
        <ReJoinOnKick>True</ReJoinOnKick>
        <ReplyOnUnknownCommandUsed>True</ReplyOnUnknownCommandUsed>
        <SendQueue>40</SendQueue>
        <UpdateTopicOnFlagAdded>True</UpdateTopicOnFlagAdded>
      </IrcConnection>
      <ServerStatus>
        <LockedColor>Orange</LockedColor>
        <OfflineColor>Red</OfflineColor>
        <OnlineColor>Green</OnlineColor>
      </ServerStatus>
    </IRCAddon>
  </Settings>
</Config>